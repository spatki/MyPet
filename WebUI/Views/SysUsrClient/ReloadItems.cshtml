@model IEnumerable<UserProfile>
@{
    Layout = null;
}
<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="processTable">
    <thead>
        <tr>
        <th>User ID</th>
        <th class="hidden-xs">Name</th>
        <th class="docTableResult">Action</th>
        </tr>
    </thead>
    <tbody id="listing">
        @foreach (var o in Model)
        {
            <tr id="@o.ID">
                <td>@o.UserName</td>
                <td>@o.DisplayName</td>
                <td><button class="btn btn-med black-bg openDialog" data-id="@o.ID" data-mode="reset" data-source="@Url.Action("ResetPassword", "SysUser", new { id = o.ID })" title="Reset Password"><i class="icon-lock"></i></button>
                    <button class="btn btn-med green-bg openDialog" data-id="@o.ID" data-mode="edit" data-source="@Url.Action("Edit", "SysUser", new { id = o.ID })"><i class="icon-">&#xf044;</i></button>
                    <button class="btn btn-med red-bg openDeleteDialog" data-id="@o.ID" data-mode="delete" data-source="@Url.Action("Delete", "SysUser", new { id = o.ID })"><i class="icon-">&#xf057;</i></button>
                </td>
            </tr>
        }
    </tbody>
</table>

