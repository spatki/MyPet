@model vw_prj_process_mapping
@{
    Layout = null;
}

<a href="javascript:;" id="option@(Model.ID)" class="loadDetails" data-element="@Model.ID" data-source="@Url.Action("ExcludeRepo","PrjProcessTailor")" 
    data-sourceid="@Model.ID" data-mode="ExcludeRepo" title = "Click to exclude">
    <i class="icon-remove"/>
</a>
<a class="tree-toggle closed" data-toggle="branch" role="branch" data-id="@Model.ID" id="Node@(Model.ID)" href="javascript:">
    <span class="label label-info">@Model.LevelShortName @Model.Name</span>
</a>
