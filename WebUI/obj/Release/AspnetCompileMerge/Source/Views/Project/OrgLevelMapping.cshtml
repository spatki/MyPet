@model ProjOrgLevelMapping
@{
    Layout = null;
}
<div class="modal-dialog">
	<div class="modal-content">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		<h4 class="modal-title" id="dialogTitle">Mapping Location to Org. Level</h4>
	</div>
    <form method="post" action="@Url.Action("OrgLevelMapping")" id="submitLevel">
        <div class="modal-body"> 
            @Html.ValidationSummary(true)
		    <div class="row">
			    <div class="col-md-12">
                    <input type="hidden" id="ID" name="ID" value="@Model.ID" />
                    <input type="hidden" id="ClientID" name="ClientID" value="@Model.ClientID" />
                    <input type="hidden" id="ProjectName" name="ProjectName" value="@Model.ProjectName" />
                    <div class="form-group">
                        <label class="control-label col-md-3"><strong>Project</strong></label>
                        <div class="col-md-9">@Model.ProjectName</div>
                    </div>
                </div>
            </div>
		    <div class="row">
			    <div class="col-md-12">
                    <div class="form-group">
                        <label class="control-label col-md-3"><strong>Org. Level</strong></label>
                        <div class="col-md-9">
                            @Html.Action("getListItems","OrgLevel",new { selectedItem = Model.LevelID, controlName = "LevelID", excludeIds = "", selectIds = "", reload = ""})
                        </div>
                        @Html.ValidationMessageFor(o => o.LevelID)
                    </div>
			    </div>	
		    </div>
	    </div>
	    <div class="modal-footer">
			<div class="form-group">
		        <button type="button" class="btn gray-bg" data-dismiss="modal">Cancel</button>
		        <button type="button" class="btn blue-bg save" data-mode="projectLocationMapping" data-id="" data-form="submitLevel">Save</button>
			</div>
	    </div>
    </form>
	</div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->

            <div class="row">
            </div>
