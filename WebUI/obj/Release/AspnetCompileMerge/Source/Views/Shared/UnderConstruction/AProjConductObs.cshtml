		});

		$("body").delegate(".addObs", "click", function () {
		    // add additional columns for edit
		    var count = parseInt($(this).parent().parent().parent().data("count"));

		    count = count + 1;
		    var newColumns = '<td class="getEditor EditObs nav" data-field="Observation" data-navkey="EditObs" data-class="EditObs"contenteditable="true"></td>' +
                             '<td  class="getEditor EditObs nav" data-field="Impact" data-navkey="EditObs" data-class="EditObs" contenteditable="true"></td>' +
                             '<td class="EditObs"  data-field="Res" data-navkey="EditObs" data-class="EditObs"></td>' +
                             '<td>Open</td>' +
                             '<td class="EditObs" data-field="Date" data-navkey="EditObs" data-class="EditObs"></td>';
		    $(this).parent().after(newColumns);
		    $(this).parent().parent().parent().data("count", count);

		    // Add the option to add more rows
		    $(this).parent().parent().parent().append('<tr id="R' + count + '" class="EditRow"><td><a href="#" class="addObs nav" data-id="R' + (count + 1) + '" ><i class="icon-">&#xf055</i></a></td></tr>');
		    $(this).removeClass("addObs");
		    $(this).addClass("removeObs");
		    $(this).children().first().html("&#xf00d");
		    $(this).parent().next().click();
		    // Go to the new rule
		    $('html, body').animate({
		        scrollTop: $(this).parent().next().offset().top
		    }, 2000);
		});


		//CKEDITOR.inline(".getEditor");

		
		$("#showfilter").click();

		$(document).on("click", "#NewTask", function () {
		    $("#reqPlanDetails").modal("show");
		});

		$('#reqPlanDetails').on('shown.bs.modal', function () {
		    if (newTaskParams) {
		        $(".chzn-select").chosen({ "disable_search_threshold": 10 });
		        $(".mask_date").inputmask("d/m/y", { "placeholder": "dd/mm/yyyy" });
		        newTaskParams = false;
		    }
		})

		$("body").delegate(".group", "click", function () {
		    $("#" + $(this).data("childid")).toggle();
		    $(this).toggleClass("closed");
		});

		$("body").delegate(".EditObs", "click", function () {
		    var field = $(this).data("field");
		    var originalValue = $(this).text();

		    switch (field) {
		        case "Observation", "Impact":
		            break;
		        case "Res":
		            keypressCode = -1;
		            keypressShift = false;
		            $(this).addClass("cellEditing");
		            $(this).removeClass("EditObs");
		            $(this).html("<select class='inputValue col-md-12' data-class='EditObs' data-navkey='EditObs'><option value='Sandeep Honawar'>Sandeep Honawar</option><option value='Kapil Khanna'>Kapil Khanna</option><option value='Supriya Phatak'>Supriya Phatak</option></select>");
		            $(this).children().first().val(originalValue);
		            $(this).children().first().focus();
		            break;
		        case "Date":
		            keypressCode = -1;
		            keypressShift = false;
                    
		            $(this).addClass("cellEditing");
		            $(this).removeClass("EditObs");
		            $(this).html("<input type='text' size='12' id='mask_date2' class='inputValue' data-class='EditObs' data-navkey='EditObs'/>");
		            $("#mask_date2").inputmask("d/m/y", { "placeholder": "dd/mm/yyyy" });
		            if (originalValue != "") $(this).children().val(originalValue);
		            $(this).children().first().focus();
		            $(this).children().first().select();
		            break;
		        default:
		            break;
		    }
		});

		$("body").delegate(".removeObs", "click", function () {
		    if (confirm("Do you want to delete this observation ? ")) {
		        $("#" + $(this).data("id")).remove();
		    }
		});

		var calculateValues = function () { };
    </script> 
   

