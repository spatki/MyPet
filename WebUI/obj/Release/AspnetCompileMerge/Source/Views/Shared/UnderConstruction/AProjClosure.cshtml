		    var newColumns = '<td class="getEditor EditObs nav" data-field="Observation" data-navkey="EditObs" data-class="EditObs"contenteditable="true"></td>' +
                             '<td  class="getEditor EditObs nav" data-field="Impact" data-navkey="EditObs" data-class="EditObs" contenteditable="true"></td>' +
