@model IEnumerable<mstr_process_type>
@{
    Layout = null;
}
@foreach (var o in Model)
{
    <tr id="@o.ID">
        <td>@o.ShortName</td>
        <td>@o.Description</td>
        <td><button class="btn btn-med green-bg openDialog" data-id="@o.ID" data-mode="edit" data-source="@Url.Action("Edit", new { id = o.ID })"><i class="icon-">&#xf044;</i></button>
            <button class="btn btn-med red-bg openDeleteDialog" data-id="@o.ID" data-mode="delete" data-source="@Url.Action("Delete", new { id = o.ID })"><i class="icon-">&#xf057;</i></button>
        </td>
    </tr>
}

