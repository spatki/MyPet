@using ProcessAccelerator.WebUI
@model IEnumerable<tbl_org_resourceplan_human>
@{
    Layout = null;
    var durationUnit = new DurationUnitTypes();
}
@foreach (var o in Model)
{
    <tr id="@o.ID">
        <td>@o.mstr_org_role.ShortName</td>
        <td>@(o.tbl_Org_Proj_LocationID != null ? o.tbl_org_proj_location.Name : "")</td>
        <td>@o.PlannedStart.ToString(Globals.DateFormatString)</td>
        <td>@o.PlannedEnd.ToString(Globals.DateFormatString)</td>
        <td>@o.Count</td>
        <td>@o.AllocationPercent</td>
        <td>@(o.tbl_Org_Proj_GroupID != null ? o.tbl_org_proj_group.Name : "")</td>
        <td>@o.CreateDate.ToString(Globals.DateFormatString)</td>
        <td>@o.Skills</td>
        <td>@o.mstr_process_lc_status.Status</td>
        <td><button class="btn btn-med green-bg openDialog" data-id="@o.ID" data-mode="edit" data-source="@Url.Action("Edit", "ProjResourcePlan", new { id = o.ID })"><i class="icon-">&#xf044;</i></button></td>
   </tr>
}