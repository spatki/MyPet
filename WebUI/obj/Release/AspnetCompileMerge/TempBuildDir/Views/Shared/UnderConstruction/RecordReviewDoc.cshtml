	<script type="text/javascript">
	    var onlyOnce = true;
	    var config = {
	        '.chzn-select': {},
	        '.chzn-select-deselect': { allow_single_deselect: false },
	        '.chzn-select-no-single': { disable_search_threshold: 10 },
	        '.chzn-select-no-results': { no_results_text: 'Oops, nothing found!' },
	        '.chzn-select-width': { width: "95%" }
	    }
	    for (var selector in config) {
	        $(selector).chosen(config[selector]);
	    }

	    $(document).on("click", "#filterTS", function () {
	        $(this).parent().children(".panel-body").eq(0).toggle("slow");
	    });

	    $("#filterTS").click();

	    $("body").delegate(".showDetails", "click", function () {
	        $("#empName").text($(this).data("emp"));
	        $("#period").text($(this).data("period"));
	        $("#viewTSDetails").modal("show");
	    });

	    $(document).on("click", "#NewRecord", function () {
	        $("#newRecording").modal("show");
	    });

        $(document).on("change","#recordType", function() {
            var recordType = $(this).val();
            var htmlString = "";

            switch (recordType)
            {
                case "Checklist":
                    htmlString = '<select class="col-md-12 recDetailValue chzn-nopadd chzn-select-no-single"><option value="Coding Checklist" selected="selected">Coding Checklist</option> <option value="Code review checklist">Code Review checklist</option> </select>';
                    $("#recordDetails").data("value","Coding Checklist");
                    break;
                case "Template":
                    htmlString = '<select class="col-md-12 recDetailValue chzn-nopadd chzn-select-no-single"><option value="Test Plan Template" selected="selected">Test Plan Template</option> <option value="PMP Template">PMP Template</option>  <option value="SQA Template">SQA Template</option></select>';
                    $("#recordDetails").data("value","Test Plan Template");
                    break;            
                case "Uploaded Doc":
                    htmlString = '<input type="file" class="styled recDetailValue">';
                    $("#recordDetails").data("value","");
                    break;            
            }
            $("#recordDetails").html(htmlString);
            if (recordType != "Uploaded Doc") $(".recDetailValue").chosen({ "disable_search_threshold": 10 });
        });

        $("body").delegate(".recDetailValue","change",function() {
            $("#recordDetails").data("value",$(this).val());
        });

        $('#newRecording').on('shown.bs.modal', function () {
            $("#selProj").chosen({ "disable_search_threshold": 10 });
            $("#selGroup").chosen({ "disable_search_threshold": 10 });
            $("#recordType").chosen({ "disable_search_threshold": 10 });
            $("#recDetailValue").chosen({ "disable_search_threshold": 10 });
            $("#filterTaskType").chosen({ "allow_single_deselect": false });
        });

        $('#linkTasks').on('shown.bs.modal', function () {
            $("#filterTaskType").chosen({ "allow_single_deselect": false });
            $("#fromDate").inputmask("d/m/y",{ "placeholder": "dd/mm/yyyy"});
            $("#toDate").inputmask("d/m/y",{ "placeholder": "dd/mm/yyyy"});
            $("#taskType").val("1");
            $("#listAssignedTasks").hide();
            $("#listTSTasksOther").show();
        });   

        $(document).on("change",".radioOption", function() {
            if ($(this).val() == 1) {
                $("#listAssignedTasks").show();
                $("#listTSTasksOther").hide();
            }
            else
            {
                $("#listAssignedTasks").hide();
                $("#listTSTasksOther").show();
            }            
        });

    $("body").delegate(".linkTask","click", function() {
        $("#newRecording").modal("show");
        $("#LTT").click();
    });
        
    $("body").delegate(".showRec","click", function() {
        $("#newRecording").modal("show");
        $("#RD").click();
    });

    $("body").delegate(".showDocumentData", "click", function () {
