            else $("#obs_sheet").show("slow");
        });

		$("body").delegate(".report", "click", function () {
		    $("#Audit_ID").val($(this).data("id"));
            $("#obs_sheet").show();
		    $("#QA_Obsv").click();
		});

		$("body").delegate(".selectTabOption", "click", function () {
            if ($(this).prop("id") == "NCForm") 
            {
                if (!$(this).parent().hasClass("active"))
                {
                    $("#AuditChkLstDetails").hide();
                    $("#NCFormDetails").show();
                    $("#AuditChkLst").parent().removeClass("active");
                    $(this).parent().addClass("active");
                    $("#formTitle").html("NC Form");
                }
            }
            else
            {
                if (!$(this).parent().hasClass("active"))
                {
                    $("#NCFormDetails").hide();
                    $("#AuditChkLstDetails").show();
                    $("#NCForm").parent().removeClass("active");
                    $(this).parent().addClass("active");
                    $("#formTitle").html("Audit Checklist");
                }
            }
        });

		$("body").delegate(".addObs", "click", function () {
		    // add additional columns for edit
		    var count = parseInt($(this).parent().parent().parent().data("count"));

		    count = count + 1;
		    var newColumns = '<td class="getEditor EditObs nav" data-field="NC" data-navkey="EditObs" data-class="EditObs"contenteditable="true"></td>' +
                             '<td  class="getEditor EditObs nav" data-field="Attribution" data-navkey="EditObs" data-class="EditObs" contenteditable="true"></td>' +
                             '<td  class="getEditor EditObs nav" data-field="CrrAction" data-navkey="EditObs" data-class="EditObs" contenteditable="true"></td>' +
                             '<td class="EditObs"  data-field="ECDate" data-navkey="EditObs" data-class="EditObs"></td>' +
                             '<td class="EditObs" data-field="Status" data-navkey="EditObs" data-class="EditObs"></td>' +
                             '<td class="EditObs" data-field="SCDate" data-navkey="EditObs" data-class="EditObs"></td>';
		    $(this).parent().after(newColumns);
		    $(this).parent().parent().parent().data("count", count);

		    // Add the option to add more rows
		    $(this).parent().parent().parent().append('<tr id="R' + count + '" class="EditRow"><td><a href="#" class="addObs nav" data-id="R' + (count + 1) + '" ><i class="icon-">&#xf055</i></a></td></tr>');
		    $(this).removeClass("addObs");d
		    $(this).addClass("removeObs");
		    $(this).children().first().html("&#xf00d");
		    $(this).parent().next().click();
		    // Go to the new rule
		    $('html, body').animate({
		        scrollTop: $(this).parent().next().offset().top
		    }, 2000);
		});


