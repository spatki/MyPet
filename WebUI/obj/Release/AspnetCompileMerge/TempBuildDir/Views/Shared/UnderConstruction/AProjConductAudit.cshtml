		$("body").delegate(".EditObs", "click", function () {
		    var field = $(this).data("field");
		    var originalValue = $(this).text();

		    keypressCode = -1;
		    keypressShift = false;
		    switch (field) {
		        case "NC":
                case "Attribution":
                case "CrrAction":
		            break;
		        case "Status":
		            $(this).addClass("cellEditing");
		            $(this).removeClass("EditObs");
		            $(this).html("<select class='inputValue col-md-12' data-class='EditObs' data-navkey='EditObs'><option value='Open'>Open</option><option value='Closed'>Closed</option><option value='Escalated'>Escalated</option><option value='Withdrawn'>Withdrawn</option></select>");
		            $(this).children().first().val(originalValue);
		            $(this).children().first().focus();
		            break;
		        case "ECDate":
                case "SCDate":                    
		            $(this).addClass("cellEditing");
		            $(this).removeClass("EditObs");
		            $(this).html("<input type='text' size='12' id='mask_date2' class='inputValue' data-class='EditObs' data-navkey='EditObs'/>");
		            $("#mask_date2").inputmask("d/m/y", { "placeholder": "dd/mm/yyyy" });
		            if (originalValue != "") $(this).children().val(originalValue);
		            $(this).children().first().focus();
		            $(this).children().first().select();
		            break;
		        case "PCI_Inc":
		            $(this).addClass("cellEditing");
		            $(this).removeClass("EditObs");
		            $(this).html("<select class='inputValue' data-class='EditObs' data-navkey='EditObs'><option value='Yes'>&nbsp;&nbsp;Yes&nbsp;&nbsp;</option><option value='No'>&nbsp;&nbsp;No&nbsp;&nbsp;</option></select>");
		            $(this).children().first().val(originalValue);
		            $(this).children().first().focus();
		            break;
		        case "PCI":
		            $(this).addClass("cellEditing");
		            $(this).removeClass("EditObs");
		            $(this).html("<select class='inputValue' data-class='EditObs' data-navkey='EditObs'><option value='FI'>&nbsp;&nbsp;FI&nbsp;&nbsp;</option><option value='LI'>&nbsp;&nbsp;LI&nbsp;&nbsp;</option><option value='PI'>&nbsp;&nbsp;PI&nbsp;&nbsp;</option><option value='NI'>&nbsp;&nbsp;NI&nbsp;&nbsp;</option><option value='NY'>&nbsp;&nbsp;NY&nbsp;&nbsp;</option><option value='NA'>&nbsp;&nbsp;NA&nbsp;&nbsp;</option></select>");
		            $(this).children().first().val(originalValue);
		            $(this).children().first().focus();
		            break;
		        default:
		            break;
		    }
		});

		$("body").delegate(".removeObs", "click", function () {
		    if (confirm("Do you want to delete this observation ? ")) {
		        $("#" + $(this).data("id")).remove();
		    }
		});

		var calculateValues = function () {
		    var pci_Count = 0;
		    var totalPC = 0;
		    var totalPC_Count = 0;
		    var phasePC = 0;
		    var pci = 0;

		    // Update individual scores
		    $("td[data-field='PCI']").each(function () {
		        $("#" + $(this).prop("id") + "_W").removeClass("Audit_NC");
		        switch ($(this).text()) {
		            case "FI":
		                $("#" + $(this).prop("id") + "_W").text("1.00");
		                $("#" + $(this).prop("id") + "_PC").children().first().text("100");
		                break;
		            case "LI":
		                $("#" + $(this).prop("id") + "_W").text("0.75");
		                if ($("#" + $(this).prop("id") + "_Inc").text() == "No") $("#" + $(this).prop("id") + "_PC").children().first().text("100");
                        else $("#" + $(this).prop("id") + "_PC").children().first().text("75");
		                break;
                    case "PI":
                        $("#" + $(this).prop("id") + "_W").text("0.50");
                        if ($("#" + $(this).prop("id") + "_Inc").text() == "No") $("#" + $(this).prop("id") + "_PC").children().first().text("100");
                        else $("#" + $(this).prop("id") + "_PC").children().first().text("50");
                        break;
                    case "NI":
                        $("#" + $(this).prop("id") + "_W").text("NC");
                        $("#" + $(this).prop("id") + "_W").addClass("Audit_NC");
                        if ($("#" + $(this).prop("id") + "_Inc").text() == "No") $("#" + $(this).prop("id") + "_PC").children().first().text("100");
                        else $("#" + $(this).prop("id") + "_PC").children().first().text("0");
                        break;
                    case "NY":
                    case "NA":
                        $("#" + $(this).prop("id") + "_W").text("");
                        $("#" + $(this).prop("id") + "_PC").children().first().text("100");
                        break;
                }
		    });

		    totalPC = 0;
		    totalPC_Count = 0;
		    $(".phaseScore").each(function () {
		        phasePC = 0;
		        pci_Count = 0
		        pci = 0
		        $("." + $(this).prop("id")).each(function () {
		            pci_Count = pci_Count + 1;
		            pci = pci + parseInt($(this).children().first().text());
		        });
		        phasePC = parseFloat(pci / pci_Count).toFixed(0);
		        totalPC = parseInt(totalPC) + parseInt(phasePC);
		        totalPC_Count = totalPC_Count + 1
		        $(this).text(phasePC + "%");
		    });

		    $("#projScore").text(parseFloat(totalPC / totalPC_Count).toFixed(0));
		};

    </script> 

