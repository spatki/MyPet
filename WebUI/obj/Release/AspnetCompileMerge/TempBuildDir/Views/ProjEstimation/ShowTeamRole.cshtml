        <input type="hidden" id="Prod@(Model.ID)tbl_Org_ProjectIDInput" name="tbl_org_proj_estm_productivity[@(Model.ID)].tbl_Org_ProjectID" value="@Model.ProjectID" />
        <input type="hidden" id="Prod@(Model.ID)tbl_Proj_EstimationIDInput" name="tbl_org_proj_estm_productivity[@(Model.ID)].tbl_Proj_EstimationID" value="@Model.Proj_EstimationID" />
        <input type="hidden" id="Prod@(Model.ID)tbl_Org_RoleIDInput" name="tbl_org_proj_estm_productivity[@(Model.ID)].tbl_Org_RoleID" value="@Model.ParameterID" />
    </td>
    <td>@Model.Name</td>
    <td id="Prod@(Model.ID)ResourceCount" class="EditNumber teamProd updateInput" data-navkey="EditNumber"></td>
    <td id="Prod@(Model.ID)ResourceCountFP" class="EditNumber updateInput" data-navkey="EditNumber"></td>
    <td id="Prod@(Model.ID)ResourceCountPC" class="EditNumber updateInput" data-navkey="EditNumber"></td>
