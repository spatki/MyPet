        <i class="icon-remove"></i></a>
        <input type="hidden" id="tbl_org_proj_estm_productivity.Index" name="tbl_org_proj_estm_productivity.Index" value="@Model.ID" />
        <input type="hidden" id="tbl_org_proj_estm_productivity[@(Model.ID)].ID" name="tbl_org_proj_estm_productivity[@(Model.ID)].ID" value="@Model.ID" />
        <input type="hidden" id="tbl_org_proj_estm_productivity[@(Model.ID)].ClientID" name="tbl_org_proj_estm_productivity[@(Model.ID)].ClientID" value="@Model.ClientID" />
        <input type="hidden" id="Prod@(Model.ID)]FP_Per_DayInput" name="tbl_org_proj_estm_productivity[@(Model.ID)].FP_Per_Day" value="" />
