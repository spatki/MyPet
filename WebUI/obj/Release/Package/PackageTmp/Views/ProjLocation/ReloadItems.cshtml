@model IEnumerable<tbl_org_proj_location>
@{
    ViewBag.Title = "ReloadItems";
    var locTypes = new ProjectLocationTypes();
}
<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="processTable">
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Type</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody id="listing">
        @foreach (var o in Model)
        {
            <tr id="@o.ID">
                <td>@o.SequenceNo</td>
                <td>@o.Name</td>
                <td>@locTypes.getTypeName(o.Type)</td>
                <td><button class="btn btn-med green-bg openDialog" data-id="@o.ID" data-mode="edit" data-source="@Url.Action("Edit", "ProjLocation", new { id = o.ID })"><i class="icon-">&#xf044;</i></button>
                    <button class="btn btn-med red-bg openDeleteDialog" data-id="@o.ID" data-mode="delete" data-source="@Url.Action("Delete", "ProjLocation", new { id = o.ID })"><i class="icon-">&#xf057;</i></button>
                </td>
            </tr>
        }
    </tbody>
</table>
