@using ProcessAccelerator.WebUI.Dto
@using Omu.AwesomeMvc
@model tbl_org_projectInput
@{
    Layout = null;
}
@Html.Action("BreadCrump", "Menu", new { id = ViewBag.FunctionID })
<div class="row col-md-12">
    <div class="pull-right"><a href="javascript:;" data-toggle="modal" class="btn btn-med blue-bg subMenu" data-id="listing" data-source="@Url.Action("Create")">Initiate New Project</a></div>
</div>
<div class="row">
    <div class="panel colored col-md-12">
        <div class="panel-heading green-bg">
            <h3 class="panel-title">Projects</h3>
            <ul class="pull-right toolbar">
                <li><a href="#" class="icon-button"><i class="icon-">&#xf059;</i></a></li>
                <li><a href="javascript:;" class="icon-button reload-box reLoad" data-container="dataList" data-url="/ProcessRole/ReloadItems"><i class="icon-">&#xf021;</i></a></li>
                <li><a href="#" class="icon-button mini-max"><i class="icon-">&#xf0aa;</i></a></li>
            </ul>
        </div>
        <div class="panel-body" id="dataList">
            <table cellpadding="0" cellspacing="0" border="0"  class="table table-striped table-bordered" id="processTable">
                <thead>
                    <tr>
                    <th>Name</th>
                    <th class="hidden-xs">Description</th>
                    <th>Type</th>
                    <th>Created By</th>
                    <th>Status</th>
                    <th>Status User</th>
                    <th class="docTableResult">Action</th>
                    </tr>
                </thead>
                <tbody id="listing">
                    @Html.Action("GetItems","Project")
                </tbody>
            </table>
        </div>
    </div>                
</div>

@section scripts {
        jsTable('processTable');
}


