        var calculateEffSch = function () {
            var CUT = $("#TCutEffPDs").text(); // CUT efforts
            var CUTPC = $("#TCutEffTotal").val(); // CUT %
            var estPD = 0;
            var estCost = 0;
            var PCTotal = 0;
            
            $("#PPC4").text(CUTPC); // Cut %

            $(".effSchEst").each(function () {
                $(this).text(parseFloat($(this).text()).toFixed(0));
                $("#" + $(this).prop("id") + "Eff").text(((parseFloat($("#TOvrllProjEff").text()) * parseFloat($(this).text()))/100).toFixed(2));
                $("#" + $(this).prop("id") + "Cost").text(((parseFloat($("#" + $(this).prop("id") + "Eff").text()) * parseFloat($("#" + $(this).prop("id") + "PerCost").text())) / 24).toFixed(0));
                estPD = estPD + parseFloat($("#" + $(this).prop("id") + "Eff").text());
                estCost = estCost + parseFloat($("#" + $(this).prop("id") + "Cost").text());
                PCTotal = PCTotal + parseInt($(this).text());

                // Update the schedule
                $("#" + $(this).prop("id") + "SchEff").text(parseFloat($("#" + $(this).prop("id") + "Eff").text()).toFixed(0));
                $("#" + $(this).prop("id") + "SchPD").text($("#" + $(this).prop("id") + "Eff").text());
            });
            $("#estEff").text(estPD);
            $("#estCost").text(estCost);
            $("#PCTotal").text(PCTotal);
            $("#daysTotal").text(estPD);
            $("#costTotal").text(estCost);
        };

