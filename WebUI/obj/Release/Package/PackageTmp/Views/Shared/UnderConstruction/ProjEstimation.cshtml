    var calculateEffort = function () {
        var prodSum = 0
        var totalSum = 0

        $(".effort").each(function () {
            prodSum = prodSum + (parseFloat($(this).text()) * parseFloat($("#" + $(this).prop("id") + "FP").text()) * (parseFloat($("#" + $(this).prop("id") + "PC").text())/100));
            totalSum = totalSum + parseFloat($(this).text());
        });

        if (totalSum) {
            $("#TProductivity").text((prodSum / totalSum).toFixed(2));
            $("#TSizeFPCut").text((parseFloat($("#Total").text()) * parseFloat($("#vaf").text())).toFixed(1));
            $("#TCutEffPDs").text((parseFloat($("#TSizeFPCut").text()) / parseFloat($("#TProductivity").text())).toFixed(1));
            $("#TOvrllProjEff").text(((parseFloat($("#TCutEffPDs").text()) / parseFloat($("#TCutEffTotal").val())) * 100).toFixed(1));
        }
    };

